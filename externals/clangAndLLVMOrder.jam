rule rtrim ( string ) 
{ 
   local match = [ MATCH "^(.*)[\n]$" : $(string) ] ; 
   if $(match) 
   { 
     return $(match[1]) ; 
   } 
   else 
   { 
     return $(string) ; 
   } 
} 
